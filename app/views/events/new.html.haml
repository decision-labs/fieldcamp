-content_for :head do
  =javascript_include_tag 'http://maps.google.com/maps/api/js?sensor=false'

= render 'js/new-map_js'

= render :partial => "shared/nav"

.block
  .content
    %h2.title
      = t(:new_event)
    .inner
      .form
        = render 'form'

= link_to raw("&larr; #{t(:back_to_current_project)} (#{@project.title})"), project_path(@project)
%br
= link_to raw("&larr; #{t(:back_to_projects)}"), projects_path
